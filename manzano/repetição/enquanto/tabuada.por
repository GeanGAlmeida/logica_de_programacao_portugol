programa
{
	
	funcao inicio()
	{
	inteiro cont = 1, num
	
		escreva("Digite um numero para saber sua tabuada: ")
		leia(num)

		enquanto (cont < 11){

			escreva(num," X ", cont, " = ", num * cont,"\n")
			cont++
		}
	}
}
